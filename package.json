{
  "name": "spaceSim",
  "version": "0.0.0",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "shippedProposals": true,
          "targets": {
            "browsers": [
              ">50%"
            ]
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "react-refresh/babel"
    ]
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "baobab": "^2.6.1",
    "baobab-react": "^4.0.2",
    "bluebird": "^3.7.2",
    "browser-bunyan": "^0.4.0",
    "classnames": "^2.3.2",
    "compression": "^1.7.4",
    "emotion": "^10.0.6",
    "express": "^4.18.2",
    "gpu.js": "^2.16.0",
    "msgpack-lite": "^0.1.26",
    "postprocessing": "^4.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^2.0.3",
    "superstruct": "^0.6.0",
    "three": "^0.89.0",
    "throttle-debounce": "^2.0.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "graceful-fs": "^4.2.10",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.1.0",
    "process": "^0.11.10",
    "raw-loader": "^4.0.2",
    "react-refresh": "^0.14.0",
    "standard": "^12.0.1",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "worker-loader": "^3.0.8"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "plugin:react/recommended"
    ],
    "globals": {
      "THREE": false,
      "OIMO": false,
      "postMessage": false,
      "self": true,
      "requestAnimationFrame": false
    },
    "rules": {
      "object-curly-spacing": [
        2,
        "always"
      ],
      "array-bracket-spacing": [
        2,
        "always"
      ],
      "func-style": [
        2,
        "expression"
      ],
      "prefer-arrow-callback": [
        2,
        {
          "allowNamedFunctions": false
        }
      ],
      "react/jsx-curly-spacing": [
        2,
        "always"
      ],
      "jsx-quotes": [
        "error",
        "prefer-single"
      ],
      "react/prop-types": 0,
      "react/display-name": 0
    }
  },
  "scripts": {
    "build": "webpack --progress",
    "clean": "rm -rf dist/*",
    "dev": "webpack-dev-server",
    "start": "node app/server.js",
    "heroku-postbuild": "NODE_ENV=production webpack -p --progress --colors"
  }
}
